{
  "configurations": [
    {
      "name": "Wrangler",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "cwd": "${workspaceFolder}",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "attachExistingChildren": false,
      "autoAttachChildProcesses": false,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/.wrangler/**/*.js",
        "${workspaceFolder}/dist-worker/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
